{
  "name": "nextjs-dashboard",
  "version": "1.0.0",
  "description": "NextJS version of Dashboard React",
  "main": "index.js",
  "scripts": {
    "start": "next start",
    "build": "next build",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && rm -rf .next/ && rm -rf out/ && npm install && npm run dev",
    "compile:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js assets/scss/nextjs-argon-dashboard.scss assets/css/nextjs-argon-dashboard.css",
    "minify:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js assets/scss/nextjs-argon-dashboard.scss assets/css/nextjs-argon-dashboard.min.css --output-style compressed",
    "map:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js assets/scss/nextjs-argon-dashboard.scss assets/css/nextjs-argon-dashboard.css --source-map true",
    "build:scss": "npm run compile:scss && npm run minify:scss && npm run map:scss",
    "dev": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CChalland/NextJS_ReactBootstrap_Theme.git"
  },
  "author": "CChalland",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CChalland/NextJS_ReactBootstrap_Theme/issues"
  },
  "homepage": "https://github.com/CChalland/NextJS_ReactBootstrap_Theme#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.2",
    "@zeit/next-css": "1.0.1",
    "@zeit/next-sass": "1.0.1",
    "bootstrap": "4.6.0",
    "chartist": "0.10.1",
    "express": "^4.17.1",
    "history": "5.0.0",
    "match-sorter": "6.1.0",
    "moment": "2.29.1",
    "next": "10.0.6",
    "next-compose-plugins": "2.2.1",
    "next-fonts": "1.4.0",
    "next-images": "1.3.1",
    "node-sass": "4.14.1",
    "node-sass-package-importer": "5.3.2",
    "nouislider": "14.6.3",
    "path": "0.12.7",
    "perfect-scrollbar": "1.5.0",
    "react": "17.0.1",
    "react-awesome-reveal": "^3.8.1",
    "react-big-calendar": "0.30.0",
    "react-bootstrap": "1.4.3",
    "react-bootstrap-sweetalert": "5.2.0",
    "react-bootstrap-wizard": "0.0.9",
    "react-chartist": "0.14.3",
    "react-datetime": "3.0.4",
    "react-dom": "17.0.1",
    "react-jvectormap": "0.0.16",
    "react-notification-alert": "0.0.13",
    "react-select": "3.2.0",
    "react-table": "7.6.3",
    "webpack": "4.44.1"
  }
}
